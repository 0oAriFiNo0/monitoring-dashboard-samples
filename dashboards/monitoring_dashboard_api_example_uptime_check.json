{
    "displayName": "Automated Uptime Check Dashboard",
    "gridLayout": {
      "columns": "1",
      "widgets": [
        {
          "title": "Uptime Check Availability",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"monitoring.googleapis.com/uptime_check/check_passed\" resource.type=\"uptime_url\" metric.label.\"check_id\"=\"gae-uptime-check\"",
                    "aggregation": {
                      "perSeriesAligner": "ALIGN_NEXT_OLDER",
                      "crossSeriesReducer": "REDUCE_COUNT_FALSE",
                      "groupByFields": []
                    }
                  },
                  "unitOverride": "1"
                },
                "plotType": "LINE",
                "minAlignmentPeriod": "60s"
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
              "scale": "LINEAR"
            },
            "chartOptions": {
              "mode": "COLOR"
            }
          }
        },
          {
            "title": "Uptime Check Latency",
            "xyChart": {
              "dataSets": [
                {
                  "timeSeriesQuery": {
                    "timeSeriesFilter": {
                      "filter": "metric.type=\"monitoring.googleapis.com/uptime_check/request_latency\" resource.type=\"uptime_url\" metric.label.\"check_id\"=\"gae-uptime-check\"",
                       "aggregation": {
                         "perSeriesAligner": "ALIGN_MEAN",
                         "crossSeriesReducer": "REDUCE_PERCENTILE_99",
                         "groupByFields": []
                        }
                      },
                      "unitOverride": "1"
                    },
                    "plotType": "LINE",
                    "minAlignmentPeriod": "300s"
                  }
                ],
                "timeshiftDuration": "0s",
                "yAxis": {
                  "label": "y1Axis",
                  "scale": "LINEAR"
                },
                "chartOptions": {
                  "mode": "COLOR"
                }
              }
          }
       ]
    }
  }