{
  "dashboardFilters": [
    {
      "filterType": "METRIC_LABEL",
      "labelKey": "job_id",
      "templateVariable": "job_id"
    }
  ],
  "displayName": "Dataflow Job",
  "mosaicLayout": {
    "columns": 2,
    "tiles": [
      {
        "height": 1,
        "widget": {
          "title": "Current number of vCPUs in use [SUM]",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "minAlignmentPeriod": "60s",
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "apiSource": "DEFAULT_CLOUD",
                  "timeSeriesFilter": {
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "crossSeriesReducer": "REDUCE_SUM",
                      "groupByFields": [
                        "metric.label.\"job_id\""
                      ],
                      "perSeriesAligner": "ALIGN_INTERPOLATE"
                    },
                    "filter": "metric.type=\"dataflow.googleapis.com/job/current_num_vcpus\" resource.type=\"dataflow_job\" ${job_id}"
                  }
                }
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
              "scale": "LINEAR"
            }
          }
        },
        "width": 1,
        "xPos": 0,
        "yPos": 0
      },
      {
        "height": 1,
        "widget": {
          "title": "Per-stage backlog in bytes [SUM]",
