                      "groupByFields": [
                        "metric.label.\"job_id\"",
                        "metric.label.\"stage\""
                      ],
                      "perSeriesAligner": "ALIGN_INTERPOLATE"
                    },
                    "filter": "metric.type=\"dataflow.googleapis.com/job/backlog_bytes\" resource.type=\"dataflow_job\" ${job_id}"
                  }
                }
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
              "scale": "LINEAR"
            }
          }
        },
        "width": 1,
        "xPos": 1,
        "yPos": 0
      },
