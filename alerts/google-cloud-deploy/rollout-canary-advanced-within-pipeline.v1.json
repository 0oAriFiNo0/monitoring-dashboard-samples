{
    "displayName": "Cloud Deploy - Rollout Canary Advanced (${PIPELINE_ID} pipeline)",
    "userLabels": {},
    "conditions": [
      {
        "displayName": "Rollout canary advanced in ${PIPELINE_ID} pipeline",
        "conditionMatchedLog": {
          "filter": "logName=\"projects/${PROJECT_ID}/logs/clouddeploy.googleapis.com%2Frollout_update\" AND resource.type=\"clouddeploy.googleapis.com/DeliveryPipeline\" AND resource.labels.pipeline_id=\"${PIPELINE_ID}\" AND jsonPayload.rolloutUpdateType=\"ADVANCED\""
        }
      }
    ],
    "alertStrategy": {
      "notificationRateLimit": {
        "period": "300s"
      },
      "autoClose": "604800s"
    },
    "combiner": "OR",
    "enabled": true,
    "notificationChannels": []
  }