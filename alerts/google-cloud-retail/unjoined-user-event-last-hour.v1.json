{
    "displayName": "Ratio of Unjoined User Events Ratio is Above Threshold for Last Hour",
    "userLabels": {},
    "conditions": [
      {
        "displayName": "Ratio of unjoined user events ratio is above the threshold for last hour",
        "conditionThreshold": {
          "filter": "metric.type=\"retail.googleapis.com/catalog/processed_event_count\" resource.type=\"retail.googleapis.com/Catalog\" metric.label.\"status\"=\"unjoined\"",
          "denominatorFilter": "UNJOINED_RESOURCE_FILTER",
          "comparison": "COMPARISON_GT",
          "thresholdValue": 0.1,
          "duration": "0s",
          "trigger": {
            "count": 1
          },
          "aggregations": [{
            "alignmentPeriod": "3600s",
            "crossSeriesReducer": "REDUCE_SUM",
            "perSeriesAligner": "ALIGN_SUM"
          }],
          "denominatorAggregations": [{
            "alignmentPeriod": "3600s",
            "crossSeriesReducer": "REDUCE_SUM",
            "perSeriesAligner": "ALIGN_SUM"
          }]
        }
      }
    ],
    "alertStrategy": {
      "autoClose": "3600s"
    },
    "combiner": "OR",
    "enabled": true,
    "notificationChannels": []
  }